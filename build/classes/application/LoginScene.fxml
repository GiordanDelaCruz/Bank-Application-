<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="462.0" prefWidth="434.0" 
			xmlns="http://javafx.com/javafx/8" 
			xmlns:fx="http://javafx.com/fxml/1"
			fx:controller="application.LoginController" >

   <children>
   		
      <Text layoutX="57.0" layoutY="90.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Log in">
         <font>
            <Font name="Century Gothic" size="46.0" />
         </font>
      </Text>
      
      <Text layoutX="57.0" layoutY="162.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Username:" />
      <Text layoutX="59.0" layoutY="206.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Password:" />
      <Text layoutX="77.0" layoutY="250.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Role:" />
      
      <TextField fx:id="usernameTextField" layoutX="153.0" layoutY="136.0" />
      <TextField fx:id="passwordTextField" layoutX="153.0" layoutY="180.0" />
      <TextField fx:id="roleTextField" layoutX="153.0" layoutY="224.0" />
      
      <Button fx:id="loginButton" layoutX="299.0" layoutY="301.0" 
      		mnemonicParsing="false" onAction="#login" text="Login" />
      
   </children>
   
</AnchorPane>
